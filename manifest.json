{
  "name": "Study Helper TikTok",
  "version": "1.0",
  "description": "Removes certain functionality in TikTok in order to help with studying",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://www.tiktok.com/*"
      ],
      "js": [
        "myscript.js"
      ]
    }
  ],
  "author": "Umer Fiaz",
  "icons": {
    "16": "/images/icon-16.png",
    "32": "/images/icon-32.png",
    "48": "/images/icon-48.png",
    "128": "/images/icon-128.png"
  }
}